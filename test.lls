function syscall4(a: Int, b: Int, c: Int, d: Int): Int {
  `
  mov rax, [rbp + 40]
  mov rdi, [rbp + 32]
  mov rsi, [rbp + 24]
  mov r15, [rbp + 16]
  mov rdx, r15
  syscall
`
}

function print(a: Ptr, b: Int): Int {
  return syscall4(1, 1, Int(a), b)
